      "conditions": [
        {
          "id": 4,
          "type": "url_pattern",
          "value": "https://ytwormate.com/*"
        }
      ],
      "actions": [
        {
          "type": "redirect",
          "value": "https://wormate.io"
        }
      ]
    }
  ]
